<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lookid_front.lookid">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Lookid"
        >
        <activity android:name=".Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp_Activity" />
        <activity android:name=".FindInfo_Activity" />
        <activity android:name=".Map_Activity" />
        <activity android:name=".Main_Activity" />
        <activity android:name=".Reservation_Activity" />
        <activity android:name=".UserInfo_Activity" />
        <activity android:name=".ResList_Activity" />
        <activity android:name=".ResInfo_Activity" />


    </application>

</manifest>